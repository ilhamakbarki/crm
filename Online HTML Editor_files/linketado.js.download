			nyelv = "rus";
		} else if ((article.includes("")) || (article.includes(""))) {
			nyelv = "hin";
		} else if ((article.includes("")) || (article.includes(""))) {
			nyelv = "ara";
		} else if ((article.includes("")) || (article.includes(""))) {
			nyelv = "kor";
		} else if ((article.includes("")) || (article.includes("")) || (article.includes(""))) {
			nyelv = "jap";
		} else if ((article.includes("")) || (article.includes("")) || (article.includes(""))) {
			nyelv = "chi";
		} else if ((article.includes("")) || (article.includes("")) || (article.includes(""))) {
