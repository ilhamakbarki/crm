var nyelv = "eng"; //English default
var mondat = "";

function pickOne(nyelvVektor) {		// egy nyelvvektorbol kivalaszt egy veletlenszeru mondatot
	var randomszam = getRandomBetween(0, nyelvVektor.length-1);
	return nyelvVektor[randomszam];
}


